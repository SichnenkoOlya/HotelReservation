package by.bsuir.archive.dao;

import by.bsuir.archive.dao.exception.DAOException;

public interface DAOMatter {

	String createMatter(String surname, String name, int age, String univer, int numberOfGroup) throws DAOException;

	String findMatter(int idMatter) throws DAOException;

	String changeMatter(int idMatter, String newSurname, String newName, int newAge, String newUniver,
			int newNumberOfGroup) throws DAOException;

}
